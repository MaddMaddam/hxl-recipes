{
  "name": "WHO",
  "title": "WHO",
  "type": "cookbook",
  "columns": [
    "#date+year",
    "#indicator+code",
    "#sex+name",
    "#indicator+value+num"
  ],
  "recipes": [
    {
      "name": "Infant Mortality Rate",
      "title": "Infant Mortality Rate",
      "description": "Probability of dying between birth and age 1 per 1000 live births",
      "type": "timeseries",
      "ingredients": {
        "aggregateFunctions": [
          "sum"
        ],
        "filtersWith": [
        {
          "#indicator+code": "MDG_0000000001"
        }
        ],
        "aggregateColumns": [
          "#date+year",
          "#sex+name"
        ],
        "valueColumns": [
          "#indicator+value+num"
        ]
      }
    },
    {
      "name": "Life Expectancy",
      "title": "Life Expectancy at Birth",
      "description": "",
      "type": "timeseries",
      "ingredients": {
        "aggregateFunctions": [
          "sum"
        ],
        "filtersWith": [
        {
          "#indicator+code": "WHOSIS_000001"
        }
        ],
        "aggregateColumns": [
          "#date+year",
          "#sex+name"
        ],
        "valueColumns": [
          "#indicator+value+num"
        ]
      }
    },
    {
      "name": "Hospital beds",
      "title": "Hospital beds (per 10000 population)",
      "description": "Number of hospital beds per 10000 population",
      "type": "timeseries",
      "ingredients": {
        "aggregateFunctions": [
          "sum"
        ],
        "filtersWith": [
        {
          "#indicator+code": "WHS6_102"
        }
        ],
        "aggregateColumns": [
          "#date+year"
        ],
        "valueColumns": [
          "#indicator+value+num"
        ]
      }
    }
  ]
}
